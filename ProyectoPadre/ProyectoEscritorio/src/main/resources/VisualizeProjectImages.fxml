<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>


<VBox alignment="CENTER" prefHeight="500.0" prefWidth="500.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/11.0.1" fx:controller="co.edu.uniquindio.project.controller.VisualizeProjectImagesController">
   <children>
      <AnchorPane prefHeight="200.0" prefWidth="200.0" VBox.vgrow="ALWAYS">
         <children>
            <AnchorPane fx:id="anchorPane" layoutX="43.0" layoutY="33.0" opacity="0.55" prefHeight="434.0" prefWidth="417.0" style="-fx-background-radius: 20px; -fx-background-color: #35206A;">
               <children>
                  <Label layoutX="141.0" layoutY="32.0" text="Images del proyecto" textFill="WHITE" />
                  <Button layoutX="14.0" layoutY="14.0" mnemonicParsing="false" onAction="#handleBackButton" style="-fx-background-color: #211442; -fx-background-radius: 20px;" text="&lt;" textFill="WHITE" />
                  <ImageView fx:id="imageView" fitHeight="247.0" fitWidth="364.0" layoutX="25.0" layoutY="67.0" pickOnBounds="true" preserveRatio="true" />
                  <Button layoutX="166.0" layoutY="358.0" mnemonicParsing="false" onAction="#handleBackImageButton" style="-fx-background-color: #211442; -fx-background-radius: 20px;" text="&lt;" textFill="WHITE" />
                  <Button layoutX="208.0" layoutY="358.0" mnemonicParsing="false" onAction="#handleNextImageButton" style="-fx-background-color: #211442; -fx-background-radius: 20px;" text="&gt;" textFill="WHITE" />
               </children>
            </AnchorPane>
         </children>
      </AnchorPane>
   </children>
</VBox>
