<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>


<VBox prefHeight="500.0" prefWidth="600.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/11.0.1" fx:controller="co.edu.uniquindio.project.controller.AddEstateAgencyController">
   <children>
      <AnchorPane prefHeight="200.0" prefWidth="200.0" VBox.vgrow="ALWAYS">
         <children>
            <AnchorPane layoutX="99.0" layoutY="12.0" opacity="0.55" prefHeight="476.0" prefWidth="403.0" style="-fx-background-radius: 20px; -fx-background-color: #35206A;">
               <children>
                  <Label layoutX="50.0" layoutY="132.0" text="Correo electrónico" textFill="WHITE" />
                  <TextField fx:id="userField" layoutX="57.0" layoutY="167.0" prefHeight="26.0" prefWidth="300.0" promptText="Escribe el correo electrónico" style="-fx-background-color: #211442; -fx-text-inner-color: #FFFFFF; -fx-background-radius: 20px;" />
                  <Label layoutX="48.0" layoutY="203.0" text="Nombre" textFill="WHITE" />
                  <Label layoutX="48.0" layoutY="266.0" text="Dirección" textFill="WHITE" />
                  <TextField fx:id="nameField" layoutX="56.0" layoutY="225.0" prefHeight="26.0" prefWidth="300.0" promptText="Escribe el nombre de la inmobiliaria" style="-fx-background-color: #211442; -fx-text-inner-color: #FFFFFF; -fx-background-radius: 20px;" />
                  <TextField fx:id="addressField" layoutX="56.0" layoutY="294.0" prefHeight="26.0" prefWidth="300.0" promptText="Escribe la dirección de la inmobiliaria" style="-fx-background-color: #211442; -fx-text-inner-color: #FFFFFF; -fx-background-radius: 20px;" />
                  <Label layoutX="51.0" layoutY="56.0" text="Codigo" textFill="WHITE" />
                  <TextField fx:id="codeField" layoutX="52.0" layoutY="87.0" prefHeight="26.0" prefWidth="300.0" promptText="Escribe el código" style="-fx-background-color: #211442; -fx-text-inner-color: #FFFFFF; -fx-background-radius: 20px;" />
                  <Label layoutX="53.0" layoutY="335.0" text="Contraseña" textFill="WHITE" />
                  <Label layoutX="106.0" layoutY="32.0" text="Agrega una nueva inmobiliaria" textFill="WHITE" />
                  <Button layoutX="104.0" layoutY="436.0" mnemonicParsing="false" onAction="#handleAddButton" style="-fx-background-color: #211442; -fx-background-radius: 20px;" text="Agregar nueva Inmobiliaria" textFill="WHITE" />
                  <Button layoutX="14.0" layoutY="14.0" mnemonicParsing="false" onAction="#handleBackButton" style="-fx-background-color: #211442; -fx-background-radius: 20px;" text="&lt;" textFill="WHITE" />
                  <PasswordField fx:id="passwordField" layoutX="52.0" layoutY="370.0" prefHeight="26.0" prefWidth="300.0" promptText="Escribe la contraseña de la inmobiliaria" style="-fx-background-color: #211442; -fx-background-radius: 20px; -fx-text-inner-color: #FFFFFF;" />
               </children>
            </AnchorPane>
         </children>
      </AnchorPane>
   </children>
</VBox>
